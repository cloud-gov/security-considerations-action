{
  "name": "security-considerations-action",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "format": "tslint --fix '{src,__tests__}/*.ts'",
    "lint": "tslint '{src,__tests__}/*.ts'",
    "package": "ncc build --license licenses.txt",
    "test": "npx tsx --test __tests__/*.test.ts",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@vercel/ncc": "^0.38.3",
    "js-yaml": "^4.1.1",
    "tslint": "^6.1.3",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3"
  }
}
